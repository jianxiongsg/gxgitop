{
	"name": "jxgitop",
	"displayName": "jxgitop",
	"description": "vscode 自动上传",
	"version": "1.1.0",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "vscode-plugin-jxgitop",
			"properties": {
				"vscodePluginDemo.jxgitop": {
					"type": "boolean",
					"default": true,
					"description": "监听package.json 自动 commit"
				}
			}
		},
		"commands": [
			{
				"command": "extension.jxgitop",
				"title": "监听package.json 自动 commit"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"@types/cross-spawn": "^6.0.1",
		"cross-spawn": "^7.0.1"
	}
}
